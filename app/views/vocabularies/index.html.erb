<h1>Vocabularies</h1>

<%= link_to "Create Vocabulary", new_vocabulary_path %>

<ul>
  <% @vocabularies.each do |vocabulary| %>
    <li>
      <%= link_to vocabulary.rdf_subject, term_path(:id => vocabulary.id) %>
    </li>
  <% end %>
</ul>
<%= paginate @vocabularies, :theme => 'twitter-bootstrap-3' %>
